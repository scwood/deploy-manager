{
  "day-by-day": {
    "imageName": "scwood/day-by-day:latest",
    "containerName": "day-by-day",
    "flags": "-p 3001:3001 -e VIRTUAL_HOST=daybyday.spncrwd.com"
  },
  "docker-webui": {
    "imageName": "pottava/docker-webui",
